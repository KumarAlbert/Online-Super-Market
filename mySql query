CREATE TABLE User(id INT NOT NULL AUTO_INCREMENT,firstName VARCHAR(40) NOT NULL,lastName VARCHAR(40) NOT NULL,email VARCHAR(40) NOT NULL, mobileNumber LONG  NOT NULL,password VARCHAR(20) NOT NULL,address VARCHAR(255) NOT NULL, PRIMARY KEY ( id ) );


CREATE TABLE Subcategory(id INT NOT NULL AUTO_INCREMENT,name VARCHAR(40) NOT NULL,categoryId INT NOT NULL, PRIMARY KEY ( id ),FOREIGN KEY(categoryId) REFERENCES Category(id) );




